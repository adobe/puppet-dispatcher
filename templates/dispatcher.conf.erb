# ************************************
# Managed by Puppet AEM Module
# ************************************

<IfModule dispatcher_module>
    DispatcherConfig            <%= @_farm_path %>/dispatcher.farms.any
    DispatcherLog               <%= @log_file %>
    DispatcherLogLevel          <%= @log_level %>
    DispatcherDeclineRoot       <%= @decline_root ? 'On' : 'Off' %>
    DispatcherUseProcessedURL   <%= @use_processed_url ? 'On' : 'Off' %>
<% if @pass_error && @pass_error == true -%>
    DispatcherPassError         1
<% elsif @pass_error -%>
    DispatcherPassError         <%= @pass_error %>
<% else -%>
    DispatcherPassError         0
<% end -%>
<% if @keep_alive_timeout -%>
    DispatcherKeepAliveTimeout  <%= @keep_alive_timeout %>
<% end -%>
<% if @keep_alive_timeout -%>
    DispatcherNoCanonURL        <%= @no_cannon_url ? 'On' : 'Off' %>
<% end -%>
</IfModule>
